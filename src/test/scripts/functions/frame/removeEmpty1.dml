A = read($1);
B = frame(data=["TRUE", "abc"], rows=nrow(A) / 2, cols=2, schema=["BOOLEAN", "STRING"])
C = frame(data=["FALSE", "null"], rows=nrow(A) / 2, cols=2, schema=["BOOLEAN", "STRING"])
D = rbind(B, C)
V = cbind(as.frame(A), D)

Vp = removeEmpty(target=V, margin=$2);
X = as.matrix(Vp[, 1:ncol(A)])
write(X, $3);
